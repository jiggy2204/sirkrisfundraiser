{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "index.js",
      "use": "@vercel/static"
    },
    {
      "src": "style.css",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/index.js"
    },
    {
      "src": "/index.js",
      "dest": "index.js"
    },
    {
      "src": "/style.css",
      "dest": "style.css"
    },
    {
      "src": "/(.*)",
      "dest": "index.html"
    }
  ]
}
